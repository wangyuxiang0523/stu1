<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fh.dao.AddrDao">
<select id="selectListById" resultType="com.fh.model.Address" parameterType="Integer">
    select * from w_address where id = #{id}
</select>
    <select id="selectListByPid" resultType="com.fh.model.Address" parameterType="Integer">
    select * from w_address where pid = #{id}
</select>
</mapper>